<Window x:Class="MailSender.GUI.MessageServiceWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:MailSender.GUI"
		mc:Ignorable="d"
		Title="" WindowStartupLocation="CenterScreen" Width="300" Height="300">
	<Grid Background="#FFF9F9E4">
		<TextBlock x:Name="TextBlock" Text="" HorizontalAlignment="Left" Margin="5" VerticalAlignment="Top" Foreground="#FF66603A" TextWrapping="Wrap" FontSize="14"/>

		<Button x:Name="btnOK" Content="OK" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="18" RenderTransformOrigin="0.431,3.248" Margin="0,0,0,20" Width="100" Click="btnOK_Click" Height="30"/>

	</Grid>
</Window>
